	<section class="section-md blog-section-one" data-aos="fade-up" data-aos-duration="800">
		<div class="container">
			<div class="pbmit-heading-subheading text-center">
				<h4 class="pbmit-subtitle">@Model.Value("homeBlogTitle")</h4>
				<h2 class="pbmit-title">@Model.Value("homeBlogSubTitle")</h2>
			</div>

			@*@{
				var selection = Umbraco.Content(Guid.Parse("6225fd59-0f21-401f-9f0a-a2a6901bee8e"))
