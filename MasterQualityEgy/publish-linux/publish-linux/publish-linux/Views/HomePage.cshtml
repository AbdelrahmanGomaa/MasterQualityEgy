			@{
				var root = Umbraco.Content(Guid.Parse("6225fd59-0f21-401f-9f0a-a2a6901bee8e"));

				var posts = root
				.ChildrenOfType("article")
				.Where(x => x.IsVisible())
				.ToList()
				.Take(4);

				var first = posts.FirstOrDefault();
				var rest = posts.Skip(1).ToList();
			}


			<div class="row pbminfotech-gap-10px">
				@if (first != null)
				{

					var firstBlogImage = first.Value<MediaWithCrops>("heroImage");
					var firstBlogDate = first.Value<DateTime>("date");

					<div class="col-md-12 col-lg-7 pbmit-blog-group-left">
						<article class="pbmit-blog-style-2">
							<div class="post-item">
								<div class="pbminfotech-box-content">
									<div class="pbmit-featured-container">
										<div class="pbmit-featured-img-wrapper">
											<div class="pbmit-featured-wrapper">
												<img src="@firstBlogImage?.MediaUrl()" class="img-fluid" alt="@first.Name()">
											</div>
										</div>
										<a class="pbmit-link" href="@first.Url()"></a>
									</div>
									<div class="pbmit-content-wrapper">
										<div class="pbmit-meta-wraper d-flex align-items-center">
											<div class="pbmit-meta-category-wrapper pbmit-meta-line">
												<div class="pbmit-meta-category">
													<a href="@first.Url()" rel="category tag">@first.Value("articleCategory")</a>
												</div>
											</div>
											<div class="pbmit-meta-date-wrapper pbmit-meta-line">
												<span class="pbmit-post-date">@firstBlogDate.ToString("dd-MMM-yyyy")</span>
											</div>
										</div>
										<h3 class="pbmit-post-title">
											<a href="@first.Url()">@first.Value("title")</a>
										</h3>
										<div class="pbmit-blog-btn">
											<a class="pbmit-button-inner" href="@first.Url()">
												<span class="pbmit-button-text">Read More</span>
												<i class="pbmit-base-icon-right-arrow"></i>
											</a>
										</div>
									</div>
								</div>
							</div>
						</article>
					</div>
				}

				<div class="col-md-12 col-lg-5 pbmit-blog-group-right">
					@foreach (var item in rest)
					{
						var BlogImage = item.Value<MediaWithCrops>("heroImage");
						var BlogDate = item.Value<DateTime>("date");
						<article class="pbmit-blog-style-2">
							<div class="post-item">
								<div class="pbminfotech-box-content">
									<div class="pbmit-featured-container">
										<div class="pbmit-featured-img-wrapper">
											<div class="pbmit-featured-wrapper">
												<img src="@BlogImage?.MediaUrl()" class="img-fluid" alt="@item.Name">
											</div>
										</div>
										<a class="pbmit-link" href="@item.Url()"></a>
									</div>
									<div class="pbmit-content-wrapper">
										<div class="pbmit-meta-wraper d-flex align-items-center">
											<div class="pbmit-meta-category-wrapper pbmit-meta-line">
												<div class="pbmit-meta-category">
													<a href="@item.Url()" rel="category tag">@item.Value("articleCategory")</a>
												</div>
											</div>
											<div class="pbmit-meta-date-wrapper pbmit-meta-line">
												<span class="pbmit-post-date">@BlogDate.ToString("dd-MMM-yyyy")</span>
											</div>
										</div>
										<h3 class="pbmit-post-title">
											<a href="@item.Url()">@item.Value("title")</a>
										</h3>
										<div class="pbmit-blog-btn">
											<a class="pbmit-button-inner" href="@item.Url()">
												<span class="pbmit-button-text">Read More</span>
												<i class="pbmit-base-icon-right-arrow"></i>
											</a>
										</div>
									</div>
								</div>
							</div>
						</article>
					}

				</div>
			</div>
		</div>
	</section>
	<!-- Blog End -->

